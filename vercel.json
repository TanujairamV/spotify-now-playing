{
  "version": 2,
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/spotify.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/spotify.js"
    },
    {
      "src": "/(.*)",
      "dest": "build/$1"
    }
  ]
}
